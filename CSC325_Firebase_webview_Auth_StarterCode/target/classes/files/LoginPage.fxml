                <Font name="Comic Sans MS" size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="passwordField" layoutX="10.0" layoutY="132.0" prefHeight="35.0" prefWidth="351.0" promptText="PASSWORD">
            <font>
                <Font name="Comic Sans MS" size="14.0" />
            </font>
        </TextField>
      <Text layoutX="140.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
         <font>
            <Font name="Comic Sans MS" size="38.0" />
         </font>
      </Text>
    </children>
