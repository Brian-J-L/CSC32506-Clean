<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="484.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csc325_firebase_webview_auth.view.RegisterScreenController">
    <children>
        <TextField fx:id="usernameField" layoutX="41.0" layoutY="110.0" prefHeight="35.0" prefWidth="351.0" promptText="USERNAME">
            <font>
                <Font name="Comic Sans MS" size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="emailField" layoutX="40.0" layoutY="161.0" prefHeight="35.0" prefWidth="351.0" promptText="EMAIL">
            <font>
                <Font name="Comic Sans MS" size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="passwordField" layoutX="40.0" layoutY="214.0" prefHeight="35.0" prefWidth="351.0" promptText="PASSWORD">
            <font>
                <Font name="Comic Sans MS" size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="phoneNumberField" layoutX="40.0" layoutY="273.0" prefHeight="35.0" prefWidth="351.0" promptText="PHONE NUMBER">
            <font>
                <Font name="Comic Sans MS" size="14.0" />
            </font>
        </TextField>
        <Button fx:id="registerButton" layoutX="118.0" layoutY="330.0" mnemonicParsing="false" onAction="#register" prefHeight="58.0" prefWidth="195.0" text="REGISTER">
            <font>
                <Font name="Elephant" size="24.0" />
            </font>
        </Button>
      <Text layoutX="132.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register">
         <font>
            <Font name="Comic Sans MS" size="42.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
